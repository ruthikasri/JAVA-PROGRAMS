import java.util.Scanner;

public class Main{
    public static void main(String args[]){
        
        Scanner sc = new Scanner( System.in);
        
        System.out.print("enter a no: ");
        int num = sc.nextInt();
        
        if(num%24==0){
            System.out.printf("The number "+num+ " is divisible by 24");
        }else{
            System.out.printf(String.format("The number "+num+ " is not divisible by 24"));
        }
                
    }
}
