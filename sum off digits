import java.util.Scanner;

public class Main{
    public static void main (String args[]){
        
        Scanner sc = new Scanner (System.in);
        
        System.out.print ("Enter the value: ");
        int n = sc.nextInt();
        int s =0;
        
        while (n>0){
            int d= n % 10;
            s=d+s;
            n= n/10;
        }   
         System.out.println(s);
    }
}

