public class Main{

public static void main (String [] args){

if(input1<0){
            return 1;
        }
        
        for (int i=2; i<input1; i++){
            if(input1 % i == 0){
                return 1;
            }
             else{
                return 2;
            }
         }
        return 1;
    }
    
}
