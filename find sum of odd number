import java.io.*;
import  java.util.Scanner;

class Main{
public static int main(int input1, input2, input3, input4, input5){
  int  count = 0;
        
        if (input1 % 2 =! 0) count++;
        if (input2 % 2 =! 0) count++;
        if (input3 % 2 =! 0) count++;
        if (input4 % 2 =! 0) count++;
        if (input5 % 2 =! 0) count++;
        return count;

    }  
}
